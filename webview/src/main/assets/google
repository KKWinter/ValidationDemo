<link href="/static/zl/miniyoutube/mescroll/mescroll.min.css" rel="stylesheet">
<script src="/static/zl/miniyoutube/mescroll/mescroll.min.js"></script>
<script type="text/javascript">
var _page = 1;
var mescroll = new MeScroll("body", {
		//第一个参数"mescroll"对应上面布局结构div的id (1.3.5版本支持传入dom对象)
		down: {
			use: false
		},
		up: {
			callback: upCallback, //下拉刷新的回调,别写成downCallback(),多了括号就自动执行方法了
			htmlLoading: '<span>Loading...</span>',
			htmlNodata: '<div"></div>',
			auto: false,
			page: {
				num: 0, //当前页 默认0,回调之前会加1; 即callback(page)会从1开始
				size: 0, //每页数据条数,默认10
			},
			noMoreSize: 5,
		},
		// toTop: {
		// 	//回到顶部按钮
		// 	src: "../img/mescroll-totop.png", //图片路径,默认null,支持网络图
		// 	offset: 300 //列表滚动1000px才显示回到顶部按钮
		// },
});
function upCallback(){
    _page ++;
    let hasNextPage = true;
    if (_page >= 8){
    	hasNextPage = false;
    }
    console.log('#'+_page);
    $.ajax({
    	type: 'GET',
    	url: '/?channel=50175&ac=ytb_hot_list&lang=en&is_ajax=1&page='+_page,
    	dataType: 'json',
    	cache:true,
    	success: function(res){
			data = res;
			if(data.length){
				var html = '';
				for(i in data) {
					var tpl = $('#list-tpl').html();
					tpl = tpl.replace('{{title}}', data[i].title);
					tpl = tpl.replace('{{thumb}}', data[i].thumb);
					tpl = tpl.replace('{{url}}', data[i].url);
					tpl = tpl.replace('{{channel}}', data[i].channel);
					tpl = tpl.replace('{{view_count}}', data[i].view_count);
					tpl = tpl.replace('{{duration}}', data[i].duration);
					html += tpl;

											if(_page == 1 && i == 2){
														html += '<div class="dd" style="display: block;"><script async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js">'+'</scr'+'ipt><!-- 50175-ytb-1 --><ins class="adsbygoogle"     style="display:block"     data-ad-client="ca-pub-8403853242384567"     data-ad-slot="1053806654"     data-ad-format="auto"     data-full-width-responsive="true"></ins><script>     (adsbygoogle = window.adsbygoogle || []).push({});'+'</scr'+'ipt></div>';
													}
									}

				$('.ytb-list .list').append(html);
			}
    		mescroll.endSuccess(data.length, hasNextPage);
    		if(!hasNextPage){
				$('.mescroll-upwarp').addClass('no-data');
    		}
    	},
    	error: function(xhr, type){
    		alert('Ajax error!');

    	}
    });

}
</script><div class="mescroll-upwarp"></div>
<script type="text/html" id="list-tpl">
    <div class="sec-item" style="display: block;">
        <a href="{{url}}" target="_blank">
            <span class="pic"><div class="img-cell"><img src="{{thumb}}"></div></span>
            <div class="tit-cell">
                <span class="tit" style="height:36px;">{{title}}</span>
                <span class="ext">
										<span class="channel">{{channel}}</span>
										<span class="view-count">{{duration}}</span>
				</span>
            </div>
        </a>
    </div>
</script>




